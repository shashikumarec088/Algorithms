#select score, dense_rank() over(order by score desc) as 'rank' from scores;


# refer t0 https://leetcode.com/problems/rank-scores/discuss/53094/Simple-Short-Fast for better
# understanding

select s.score, count(s2.score) as 'rank' from scores s,
(select distinct score as score from scores) s2
where s.score<=s2.score group by s.id
order by score desc;
