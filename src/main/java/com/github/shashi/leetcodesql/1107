# Write your MySQL query statement below
select login_date,
count(*) as user_count
from
(select user_id,
min(activity_date) as login_date
from traffic
where activity = 'login'
group by user_id
having min(activity_date)
between date_sub('2019-06-30', interval 90 day) and '2019-06-30')t
group by login_date
