# Write your MySQL query statement below

select l1.log_id as start_id,
min(l2.log_id) end_id
from
((select log_id from logs
where log_id-1 not in(select log_id from logs)) l1,
(select log_id from logs
where log_id+1 not in(select log_id from logs)) l2)
where l1.log_id<=l2.log_id
group by l1.log_id

/*
select min(log_id) as start_id,
max(log_id) as end_id
from
(select log_id,
row_number() over(order by log_id) as rn
from logs)
sub1
group by (sub1.log_id - sub1.rn)
order by start_id
*/