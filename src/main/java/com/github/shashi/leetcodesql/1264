# Write your MySQL query statement below

select distinct page_id as recommended_page
from friendship f
join (
select * from likes l
     where page_id not in(select
                              page_id from
                              likes where user_id=1)
) l
on ((f.user2_id = l.user_id and f.user1_id =1)
or (f.user1_id = l.user_id and f.user2_id =1))
